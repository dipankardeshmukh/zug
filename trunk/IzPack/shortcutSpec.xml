<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>
<shortcuts>
    <defaultCurrentUser/>
    <programGroup defaultName="Spark-1.1.0" location="applications" />
    <shortcut
	name="Spark" 
	target="${INSTALL_PATH}${FILE_SEPARATOR}SPARK${FILE_SEPARATOR}spark.bat" 
	workingDirectory="$INSTALL_PATH" 
	iconFile="${INSTALL_PATH}${FILE_SEPARATOR}SPARK${FILE_SEPARATOR}Images${FILE_SEPARATOR}Spark.ico"
	iconIndex="0" 
	initialState="minimized"
	programGroup="yes" 
	desktop="yes" 
	applications="no" 
	startMenu="yes" 
	startup="no">
	<createForPack name="SPARK"/>
	<os family="windows"/>
    </shortcut>
	
	 <shortcut
     name="Remove Spark"
	 target="${INSTALL_PATH}${FILE_SEPARATOR}SPARK${FILE_SEPARATOR}Uninstaller${FILE_SEPARATOR}UninstallSpark.jar"
	 workingDirectory="$INSTALL_PATH"
	 iconFile="${INSTALL_PATH}${FILE_SEPARATOR}SPARK${FILE_SEPARATOR}Images${FILE_SEPARATOR}Remove_Spark.ico"
	 iconIndex="0"
	 initialState="noShow"
	 programGroup="yes" 
     desktop="no"
     applications="no"
	 startMenu="no"
     startup="no">
     <createForPack name="SPARK" />
	 <os family="windows"/>
     </shortcut>
    
</shortcuts>
    